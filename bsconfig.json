{
  "name": "fragment-experiment",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    },
    {
      "module": "commonjs",
      "in-source": false
    }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": ["reason-react", "rescript-apollo-client"],
  "warnings": {
    "error": "+101"
  },
  "ppx-flags": [
    "@baransu/graphql_ppx_re/ppx6",
    "@reasonml-community/graphql-ppx/ppx"
  ],
  "bsc-flags": [
    "-bs-super-errors",
    "-w @a-102-30-40-42-45-48-4-9-3",
    "-open Belt",
    "-bs-D COMPILE_TARGET=\"ES6\""
  ],
  "namespace": true,
  "reason": {
    "react-jsx": 3
  },
  "graphql": {
    "apolloMode": true,
    "schema": "clientapi.json",
    "extendMutation": "ApolloClient.GraphQL_PPX.ExtendMutation",
    "extendQuery": "ApolloClient.GraphQL_PPX.ExtendQuery",
    "extendSubscription": "ApolloClient.GraphQL_PPX.ExtendSubscription",
    "templateTagImport": "gql",
    "templateTagLocation": "@apollo/client",
    "templateTagReturnType": "ApolloClient.GraphQL_PPX.templateTagReturnType"
  }
}
